{
  "extends": ["react-app", "react-app/jest", "plugin:storybook/recommended"],
  "rules": {
    "complexity": [
      "error",
      {
        "max": 15
      }
    ],
    "import/no-anonymous-default-export": "off",
    "prefer-const": "error"
  },
  "overrides": [
    {
      "files": ["**/*.stories.*"],
      "rules": {
        "import/no-anonymous-default-export": "off"
      }
    }
  ]
}
